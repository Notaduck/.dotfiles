{
	"coc.preferences.formatOnSaveFiletypes": ["js", "jsx"],
	"eslint.run": "onType",
	"eslint.autoFix": true,
	"languageserver": {
			"ccls": {
				"command": "ccls",
				"filetypes": ["c", "cpp", "objc", "objcpp"],
				"rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
				"initializationOptions": {
					 "cache": {
						 "directory": "/tmp/ccls"
					 }
				 }
			}
		},
		"java.home": "/usr/lib/jvm/java-11-openjdk/"

}
