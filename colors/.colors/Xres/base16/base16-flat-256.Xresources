! Base16 Flat
! Scheme: Chris Kempson (http://chriskempson.com)

#define base00 #2c3e50
#define base01 #34495e
#define base02 #7f8c8d
#define base03 #95a5a6
#define base04 #bdc3c7
#define base05 #e0e0e0
#define base06 #f5f5f5
